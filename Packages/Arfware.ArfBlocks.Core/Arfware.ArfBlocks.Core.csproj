<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>

		<PackageVersion>1.1.1</PackageVersion>
		<Authors>Arfware</Authors>
		<Company>Arfware</Company>
		<Product>Arfware ArfBlocks</Product>
		<Description>ArfBlocks (Cross-Blocks) is an advanced Mediator-Pattern library for Dotnet.</Description>
		<Copyright>Copyright Â© Arfware</Copyright>
		<Summary>ArfBlocks (Cross-Blocks) is an advanced Mediator-Pattern library for Dotnet.</Summary>
		<NeutralLanguage>en-US</NeutralLanguage>
		<Title>Arfware.ArfBlocks.Core</Title>
		<PackageId>Arfware.ArfBlocks.Core</PackageId>
		<PackageTags>Arfware,ArfBlocks,Mediator</PackageTags>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageIcon>icon.png</PackageIcon>
		<PackageProjectUrl>https://github.com/Arfware/arfblocks-dotnet</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Arfware/arfblocks-dotnet</RepositoryUrl>
		<RootNamespace>Arfware.ArfBlocks.Core</RootNamespace>
		<AssemblyName>Arfware.ArfBlocks.Core</AssemblyName>
		<PackageOutputPath>./nupkg</PackageOutputPath>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../LICENSE.txt" Pack="true" PackagePath="" />
		<None Include="../icon.png" Pack="true" PackagePath="" />
		<None Include="./README.md" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore" Version="2.2.0" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
		<PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.24.0" />
		<PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.24.0" />
	</ItemGroup>

</Project>
